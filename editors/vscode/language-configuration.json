{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"]
  ],
  "autoClosingPairs": [
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "{", "close": "}" },
    { "open": "\"", "close": "\"", "notIn": ["string"] },
    { "open": "${", "close": "}" }
  ],
  "surroundingPairs": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"],
    ["\"", "\""]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(if\\s*\\(.*\\)\\s*\\{\\s*$|folder\\s*\\(.*\\)\\s*\\{\\s*$|file_properties\\s*\\(.*\\)\\s*\\{\\s*$|.*\\{\\s*$)",
    "decreaseIndentPattern": "^\\s*\\}\\s*$"
  },
  "folding": {
    "markers": {
      "start": "^\\s*\\[",
      "end": "^(?=\\s*\\[)"
    }
  },
  "wordPattern": "[a-zA-Z_][a-zA-Z0-9_]*"
}
